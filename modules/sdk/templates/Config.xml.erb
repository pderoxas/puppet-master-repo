<?xml version="1.0" encoding="utf-8"?>
<Configuration xmlns="http://merchantretailsdk.paypal.com/v1.0/">
  <PayPal.Uri.Settings>
    <openIdService.baseURI><%= @openIdServiceBaseURI %></openIdService.baseURI>
    <hereAPIService.baseURI><%= @hereAPIServiceBaseURI %></hereAPIService.baseURI>
    <posAPIService.baseURI><%= @posAPIServiceBaseURI %></posAPIService.baseURI>
    <ecAPIService.baseURI><%= @ecAPIServiceBaseURI %></ecAPIService.baseURI>
  </PayPal.Uri.Settings>
  <PayPal.Retailer.Settings>
    <Store id="<%= @store_number %>">
      <payerId><%= @payerId %></payerId>
      <locationId><%= @locationId %></locationId>
      <!-- SOAP Credentials -->
      <userId><%= @userId %></userId>
      <password><%= @password %></password>
      <signature><%= @signature %></signature>
      <!-- REST Credentials for "pramana-pph1@paypal.com" -->
      <refreshToken><%= @refreshToken %></refreshToken>
      <clientSecret><%= @clientSecret %></clientSecret>
    </Store>
  </PayPal.Retailer.Settings>
</Configuration>
